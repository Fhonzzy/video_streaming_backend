{
  "name": "video-streaming-backend",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "dev": "npm run build && concurrently \"npx tsc -w\" \"nodemon --exec ts-node src/app.ts\"",
    "build": "rimraf dist && npx tsc",
    "start": "npm run build && node ./dist/app.js",
    "migrate:up": "ts-node src/database/migrator.ts up",
    "migrate:down": "ts-node src/database/migrator.ts down",
    "migrate:latest": "ts-node src/database/migrator.ts latest",
    "db:types": "kysely-codegen --dialect postgres --camel-case --out-file src/types/types.d.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.8",
    "multer": "^2.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "concurrently": "^9.2.1",
    "dbmate": "^2.28.0",
    "kysely-codegen": "^0.19.0",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
